{
    "xsappname": "uatpayrollreport",
    "tenant-mode": "dedicated",
    "scopes": [
        {
            "name": "$XSAPPNAME.PayrollAdministrator",
            "description": "Administrate the application"
        },
        {
            "name": "$XSAPPNAME.PayrollUser",
            "description": "Use the application"
        },
        {
            "name": "uaa.user",
            "description": "UAA"
        }
    ],
    "role-templates": [
        {
            "name": "Token_Exchange",
            "description": "UAA",
            "scope-references": [
                "uaa.user"
            ]
        },
        {
            "name": "PayrollAdministrator",
            "description": "PayrollAdministrator",
            "scope-references": [
                "uaa.user",
                "$XSAPPNAME.PayrollAdministrator"
            ]
        },
        {
            "name": "PayrollUser",
            "description": "PayrollUser",
            "scope-references": [
                "uaa.user",
                "$XSAPPNAME.PayrollUser"
            ]
        }
    ],
    "role-collections": [
        {
            "name": "Payroll_Administrator",
            "description": "Payroll Administrator",
            "role-template-references": [
                "$XSAPPNAME.PayrollAdministrator",
                "$XSAPPNAME.PayrollUser"
            ]
        },
        {
            "name": "Payroll_User",
            "description": "Payroll User",
            "role-template-references": [
                "$XSAPPNAME.PayrollUser"
            ]
        }
    ],
    "oauth2-configuration": {
        "token-validity": 3600,
        "redirect-uris": [
            "https://*-*.cfapps.ca10.hana.ondemand.com/**"
        ],
        "allowedproviders ": ["httpshcm47.sapsf.com"]
    }
}